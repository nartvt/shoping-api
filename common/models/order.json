    },
    "products": {
      "type": "hasMany",
      "model": "Product",
      "foreignKey": "orderId",
      "through": "OrderItem"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
